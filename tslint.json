{
    "extends": ["tslint:latest", "tslint-config-prettier", "tslint-immutable"],
    "rules": {
        "no-implicit-dependencies": [true, "dev"],
        "no-var-keyword": true,
        "no-parameter-reassignment": true,
        "typedef": [true, "call-signature"],
        "readonly-keyword": true,
        "no-let": true,
        "no-mixed-interface": true
    }
}